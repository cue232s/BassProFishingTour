<div class="loginForm">
	<div class="bassLoginLogo">
		<img class="bassLoginLogo" src="/assets/images/bassLoginLogo.png" alt="Bass Logo">
	</div>
	<%= form_tag("/login", method: "get") do %>
		<%= label_tag(:email) %>
		<%= text_field_tag(:email) %>

		<%= label_tag(:password) %>
		<%= text_field_tag(:password) %>

		<button class="submit" type="submit">Register</button>
	<% end %>
</div>